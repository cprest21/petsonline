<%- model_class = PetTreatment -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:medical_history_no) %>:</strong></dt>
  <dd><%= @pet_treatment.medical_history_no %></dd>
  <dt><strong><%= model_class.human_attribute_name(:pet_id) %>:</strong></dt>
  <dd><%= @pet_treatment.pet_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:treatment_id) %>:</strong></dt>
  <dd><%= @pet_treatment.treatment_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              pet_treatments_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_pet_treatment_path(@pet_treatment), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              pet_treatment_path(@pet_treatment),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
