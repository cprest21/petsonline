<%- model_class = PetTreatment -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:medical_history_no) %></th>
      <th><%= model_class.human_attribute_name(:pet_id) %></th>
      <th><%= model_class.human_attribute_name(:treatment_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @pet_treatments.each do |pet_treatment| %>
      <tr>
        <td><%= link_to pet_treatment.id, pet_treatment_path(pet_treatment) %></td>
        <td><%= pet_treatment.medical_history_no %></td>
        <td><%= pet_treatment.pet_id %></td>
        <td><%= pet_treatment.treatment_id %></td>
        <td><%=l pet_treatment.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_pet_treatment_path(pet_treatment), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      pet_treatment_path(pet_treatment),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_pet_treatment_path,
            :class => 'btn btn-primary' %>
